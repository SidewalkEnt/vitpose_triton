apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  name: triton-servicemonitor
  labels:
    release: prometheus
spec:
  selector:
    matchLabels:
      app: triton-server
  endpoints:
  - port: metrics
    interval: 15s
    path: /metrics
  namespaceSelector:
    matchNames:
    - default
